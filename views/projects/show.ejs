


<h3><%= proj.name %> at <%= journal.name %></h3>
<h3><%= proj.grade %></h3>
<h4><%= proj.startDate %></h4>
<% if (proj.endDate){ %>
<h4><%= proj.endDate %></h4>
<% } %>
<h3><%= proj.description %></h3>

<%= proj.notes.length %>

<% if(proj.notes.length > 0) { %>
    <% proj.notes.forEach(function(n, idx) { %>
        ---------------------------------------------
        <p> yo </p>
         ---------------------------------------------
        <br>
    <% }) %>
<% } else { %>
    <p>There are no notes for this project :(</p>
<% } %>

<form type="text" name="notes" action="/projects/<%= proj._id %>" method="POST">
<textarea id="" cols="30" rows="5" placeholder="Add notes..."></textarea><br>
<input  type="submit" value="ADD NOTE!">
</form>



